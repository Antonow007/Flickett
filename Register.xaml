<Window x:Class="Flickett.Register"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Flickett"
        xmlns:userControls="clr-namespace:Flickett.View.UserControls"
        mc:Ignorable="d"
        Title="Register" Height="400" Width="300">
    <Grid>


        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>

        </Grid.RowDefinitions>


        <userControls:textBoxes Grid.Row="1"/>
        <userControls:textBoxes Grid.Row="2"/>
        <userControls:textBoxes Grid.Row="3"/>
        <userControls:textBoxes Grid.Row="4"/>
        <userControls:textBoxes Grid.Row="5"/>
        <userControls:textBoxes Grid.Row="6"/>

    </Grid>
</Window>
